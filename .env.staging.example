# Copy this to the staging server as .env.staging and fill real secrets/hosts

COMPOSE_PROJECT_NAME=traccar-staging

# Domains
TRAEFIK_DASHBOARD_HOST=staging.ingress.socratec.gmbh
TRACCAR_HOST=staging.smc.socratec.gmbh
PGADMIN_HOST=staging.pgadmin.socratec.gmbh

# ACME / Let's Encrypt
ACME_EMAIL=you@example.com
ACME_RESOLVER=myresolver
ACME_STORAGE=/etc/traefik/acme.json

# Images (staging branch build)
TRACCAR_IMAGE=ghcr.io/socratec-telematic-gmbh/traccar-docker:staging
FORWARDER_IMAGE=ghcr.io/socratec-telematic-gmbh/traccar-forwarder:staging

# MariaDB (staging-only; DO NOT reuse production creds)
MARIADB_ROOT_PASSWORD=change_me_root
MARIADB_DATABASE=traccar
MARIADB_USER=traccar
MARIADB_PASSWORD=change_me_user

# Forwarder
ASPNETCORE_ENVIRONMENT=Staging